{
  "name": "mohiohio/graphql-wp",
  "description": "A GraphQL endpoint for WordPress",
  "type": "wordpress-plugin",
  "license": "GPL",
  "authors": [
    {
      "name": "Tim Field",
      "email": "field.tim@gmail.com"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/rocket-monkey/graphql-php.git"
    }
  ],
  "require": {
    "php": ">=5.6",
    "mohiohio/wordpress-lib": "^0.1",
    "webonyx/graphql-php": "dev-patchfix-directive-locations",
    "ivome/graphql-relay-php": "^0.1.2",
    "danielstjules/stringy": "^2.3"
  },
  "autoload": {
    "psr-4": {
      "Mohiohio\\GraphQLWP\\": "/"
    }
  },
  "require-dev": {
    "analog/analog": "^1.0"
  }
}
